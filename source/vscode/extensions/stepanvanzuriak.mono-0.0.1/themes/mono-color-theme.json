  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#BBBBBB33"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#BBBBBB88"
      }
    },
    {
      "scope": ["entity", "strong"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#BBBBBB88",
        "fontStyle": "italic underline"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
